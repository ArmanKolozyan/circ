def match(u32[4] a, u32[4] b) -> u32:
    u32 r = 0x00000000
    for i in 0..4 do 
        u32 t = a[i] - b[i]
        r = r + t * t
    endfor
    return r

def main(private<1> u32 x, private<2> u32 y) -> u32:
    N = 0x0000000a
    K = 0x00000004

    

    u32 best_match = match(x[0], y)

    for u32 i in 0..N do 
        u32 res = match(x[i], y)
        if res < best_match then best_match = res fi
    endfor

    return best_match

